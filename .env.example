# LLM Provider Configuration
# Options: 'openai' or 'anthropic'
LLM_PROVIDER=openai

# OpenAI API Key (required if LLM_PROVIDER=openai)
OPENAI_API_KEY=your_openai_api_key_here

# Anthropic API Key (required if LLM_PROVIDER=anthropic)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# LangSmith Tracing Configuration (Optional)
# Enable LangSmith for monitoring and debugging LangChain calls

# LangSmith Tracing Configuration (Optional)
# Enable LangSmith for monitoring and debugging LangChain calls
# Sign up at https://smith.langchain.com to get your API key
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=chefli-agents
